{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "uniqueItems": true,
  "items": {
    "properties": {
      "id": {
        "type": "string",
        "description": "Directory name"
      },
      "title": {
        "type": "string",
        "description": "Title of music"
      },
      "artists": {
        "type": "array",
        "uniqueItems": true,
        "items": {
          "type": "string",
          "description": "Artist of music"
        }
      },
      "groups": {
        "type": "array",
        "uniqueItems": true,
        "items": {
          "type": "string",
          "description": "Groups of artists"
        }
      },
      "length": {
        "type": "number",
        "description": "Length of music in seconds"
      }
    },
    "required": ["id", "title", "artists", "groups", "length"]
  }
}
