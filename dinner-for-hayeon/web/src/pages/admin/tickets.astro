---
import AdminLayout from "@/layouts/AdminLayout.astro";
import TicketAdjustment from "@/components/admin/TicketAdjustment.tsx";
import { getSessionUser } from "@/utils/auth.ts";

export const prerender = false;

const sessionUser = await getSessionUser(Astro.request);
---

<AdminLayout title="티켓 관리 - 관리자 페이지" activeTab="tickets">
  <TicketAdjustment sessionUser={sessionUser!} client:load />
</AdminLayout>
